<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.with.you.dao.NoteDao">

    <select id="selectList" resultType="com.with.you.entity.NoteEntity">
        SELECT *
        FROM `note`
        order BY id DESC
    </select>

    <insert id="insert">
        INSERT `note` (
            `note_id`, `title`, `text`, `time`, `img`
        ) VALUE (
            #{noteId}, #{title}, #{text}, #{time}, #{img}
        )
    </insert>

</mapper>